        {
          "tipo": "sleep",
          "valor": "2000"
        },
        {
          "tipo": "check",
          "seletor": "#btnCalcular",
          "mensagem": "Período disponível no ${unidade}: ${dataInicial} - ${dataFinal}"
        }
      ],
